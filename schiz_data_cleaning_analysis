#OTHER SPECIFIED SCHIZOPHRENIA SPECTRUM AND OTHER PSYCHOTIC DISORDER
#SCHIZOPHRENIA  DISORGANIZED TYPE UNSPECIFIED

#Create a unspecified schiz variable
library(prettyR)

eoc_dat$id_eoc = paste0(eoc_dat$client_id, eoc_dat$EOC)
eoc_dat = eoc_dat = eoc_dat %>% distinct(id_eoc, .keep_all = TRUE)
eoc_dat = subset( eoc_dat, service_date >= "2019-07-01" & service_date <= "2020-08-01")
eoc_dat = eoc_dat[order(eoc_dat$client_id),]
eoc_dat[c("client_id", "service_date", "EOC")]
eoc_dat$id_date = paste0(eoc_dat$client_id, eoc_dat$service_date)

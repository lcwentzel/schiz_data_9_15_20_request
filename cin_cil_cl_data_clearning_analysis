cin_cil_cfl_SCHIZOPHRENIA_county_by_month =  month_by_diag_fun(dat_SCHIZOPHRENIA)
cin_cil_cfl_SCHIZOPHRENIA_county_by_month$sum = apply(cin_cil_cfl_SCHIZOPHRENIA_county_by_month[-1], 1, mean)
cin_cil_cfl_SCHIZOPHRENIA_county_by_month = cin_cil_cfl_SCHIZOPHRENIA_county_by_month[order(-cin_cil_cfl_SCHIZOPHRENIA_county_by_month$sum),]
